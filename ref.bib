@article{Idahosa2016,
abstract = {Laboratory core flood experiments were conducted at different flow rates with partially hydrolysed polyacrylamide (HPAM) oilfield Enhanced Oil Recovery (EOR) polymer and silica sand to investigate the polymer retention in porous media due to flow rate variation. Specifically, the double-polymer bank method was used in a new way to quantify and understand total incremental retention (both reversible and irreversible) induced by flow rate variation for HPAM polymers. Experimental results indicate that adsorption was the dominant retention mechanism. Further, the results obtained show that polymer adsorption was rate-dependent (i.e., as flow rate increased, adsorption increased), and that the adsorption was largely reversible with minimal incremental irreversible adsorption. It was also observed that flow rate impacted polymer inaccessible pore volume (IAPV) decreasing from 32{\%} to 15{\%} as flow rate increased from 0.8ml/min to 6.0ml/min. Finally, results from the study also give better insight into understanding HPAM flow-induced adsorption and their effect on permeability reduction processes.},
author = {Idahosa, P.E.G. and Oluyemi, G.F. and Oyeneyin, M.B. and Prabhu, R.},
doi = {10.1016/J.PETROL.2016.02.020},
file = {:C$\backslash$:/Users/najafbah/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Idahosa et al. - 2016 - Rate-dependent polymer adsorption in porous media.pdf:pdf},
issn = {0920-4105},
journal = {Journal of Petroleum Science and Engineering},
mendeley-groups = {Polymer},
month = {jul},
pages = {65--71},
publisher = {Elsevier},
title = {{Rate-dependent polymer adsorption in porous media}},
url = {https://www.sciencedirect.com/science/article/pii/S0920410516300638?via{\%}3Dihub},
volume = {143},
