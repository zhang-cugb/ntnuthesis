file = {:C$\backslash$:/Users/bahad/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ayatollahi, Zerafat - 2012 - Nanotechnology-Assisted EOR Techniques New Solutions to Old Challenges(2).pdf:pdf},
isbn = {9781622761104},
mendeley-groups = {General Explanations (EOR/nano)},
pages = {1--15},
title = {{Nanotechnology-Assisted EOR Techniques: New Solutions to Old Challenges}},
year = {2012}
}

